curl -s \
  -X POST "http://localhost:7002/api/account/change_name" \
  -H "Content-Type: application/json" \
  -H "Cookie: scarif_access_token=$(jq -r '."test-access-token"' ~/my/holocron.json)" \
  -d '{"name":"newTestName"}' | jq


curl -s \
  -X POST "http://localhost:7002/api/account/change_name" \
  -H "Content-Type: application/json" \
  -d '{"name":"newTestName"}' | jq


curl -s \
  -X POST "http://localhost:7002/api/account/change_name" \
  -H "Content-Type: application/json" \
  -H "Cookie: scarif_access_token=$(jq -r '."test-access-token"' ~/my/holocron.json)" \
  -d '{"name":"ab"}' | jq
